        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Move, turn and shoot

// Move the player ////////////////////////////////////////////////////////////////////////////

// Using the left axis controller
var h_move = gamepad_axis_value(0, gp_axislh);
var v_move = gamepad_axis_value(0, gp_axislv);

if h_move != 0 || v_move != 0
{
x += h_move * 6;
y += v_move * 6;
}

// Using the D-Pad
/*
var h_move = gamepad_button_check(pad_num, gp_padr) - gamepad_button_check(pad_num, gp_padl)
var v_move = gamepad_button_check(pad_num, gp_padd) - gamepad_button_check(pad_num, gp_padu)

if ((abs(h_move) &gt; 0) || (abs(v_move) &gt; 0))
{
x += h_move * 4;
y += v_move * 4;
}
*/

// Turn the player ////////////////////////////////////////////////////////////////////////////
var h_point = gamepad_axis_value(0, gp_axisrh);
var v_point = gamepad_axis_value(0, gp_axisrv);
if ((abs(h_point) &gt; 0) || (abs(v_point) &gt; 0))
{
var pdir = point_direction(0, 0, h_point, v_point);
var dif = angle_difference(pdir, image_angle);
image_angle += median(-20, dif, 20);
}

// Shoot bullets ////////////////////////////////////////////////////////////////////////////
/*var r_trig = gamepad_button_value(0, gp_shoulderrb);
var rate = 1 - r_trig;
if can_shoot &amp;&amp; rate &lt; 1
{
with (instance_create(x, y, objectbullet))
    {
    speed = 10;
    direction = other.image_angle;
    image_angle = direction;
    }
can_shoot = false;
alarm[1] = max(5, (room_speed * rate));
}
*/
///Weapon Select////////////////////////////////////////////////////////////////////////////////
if (gamepad_button_value(0, gp_face4))
{
    instance_destroy(objectMLaser);
    instance_destroy(objectMShot);
    instance_destroy(objectMMace);
    instance_destroy(objectMPulse);
    instance_create(self.x,self.y,objectMStream);
}
if (gamepad_button_value(0, gp_face3))
{
    instance_destroy(objectMLaser);
    instance_destroy(objectMShot);
    instance_destroy(objectMStream);
        instance_destroy(objectMPulse);
    instance_create(self.x,self.y,objectMMace);
}
if (gamepad_button_value(0, gp_face2))
{
    instance_destroy(objectMLaser);
    instance_destroy(objectMMace);
    instance_destroy(objectMStream);
        instance_destroy(objectMPulse);
    instance_create(self.x,self.y,objectMShot);
}
if (gamepad_button_value(0, gp_face1))
{
    instance_destroy(objectMShot);
    instance_destroy(objectMMace);
    instance_destroy(objectMStream);
    instance_destroy(objectMPulse);
    instance_create(self.x,self.y,objectMLaser);
}
if (gamepad_button_value(0, gp_shoulderlb))
{
    instance_destroy(objectMShot);
    instance_destroy(objectMMace);
    instance_destroy(objectMStream);
    instance_create(self.x,self.y,objectMPulse);
}
if(gamepad_button_check_released(0,gp_shoulderr) &amp;&amp; (instance_exists(objectMLaser)))
{
    audio_stop_sound(LaserShot);
    instance_destroy(objectPL);
}
////////////////////////////////////////////////////////////////////////////////////
if ((gamepad_button_value(0, gp_shoulderr))&amp;&amp; (instance_exists(objectMStream)))
{
    var inst;
        audio_play_sound(UpdatedStream, 10, false);
        alarm[1] = max(3, (room_speed));
        if(global.Energy &lt;= global.Energymax)
        {
            global.Energy += 5;
        }
        inst = instance_create(x, y, objectbullet);
        with (inst)
        {
        speed = 20;
        direction = other.image_angle;
        }
}
if((instance_exists(objectMShot)) &amp;&amp; (gamepad_button_value(0, gp_shoulderr)) &amp;&amp; (global.Energy &gt;= 0))
{
    var inst, K, L;
    global.Energy -= 30;
    audio_play_sound(ShotgunShot, 10, false);
    inst = instance_create(x, y, objectbullet);
    with (inst)
    {
        speed = 20;
        direction = other.image_angle;
    }
   K = instance_create(x, y, objectbullet);
with (K)
   {
   speed = 20;
   direction = other.image_angle+15;
   }
   L = instance_create(x, y, objectbullet);
with (L)
   {
   speed = 20;
   direction = other.image_angle-15;
   }
}
if((instance_exists(objectMLaser))&amp;&amp;(gamepad_button_value(0, gp_shoulderr)) &amp;&amp; (global.Energy &gt;= 0))
{
    var inst;
    audio_play_sound(LaserShot,10,true);
    inst = instance_create(x+5, y+5, objectPL);
    alarm[0] = room_speed;
}

if ((gamepad_button_value(0, gp_shoulderrb))&amp;&amp; (global.Energy &gt;= 0))
{
var inst, K, L,j,o,p,e,s;
global.Energy -= 50;
audio_play_sound(SingleMultiShot, 10, false);
inst = instance_create(x, y, blockBullet);
with (inst)
   {
   speed = 20;
   direction = 0;
   }
   K = instance_create(x, y, blockBullet);
with (K)
   {
   speed = 20;
   direction = 45;
   }
   L = instance_create(x, y, blockBullet);
with (L)
   {
   speed = 20;
   direction = 90;
   }
    j = instance_create(x, y, blockBullet);
with (j)
   {
   speed = 20;
   direction = 135;
   }
    o = instance_create(x, y, blockBullet);
with (o)
   {
   speed = 20;
   direction = 180;
   }
    p = instance_create(x, y, blockBullet);
   with (p)
   {
   speed = 20;
   direction = 225;
   }
    e = instance_create(x, y, blockBullet);
with (e)
   {
   speed = 20;
   direction = 270;
   }
    s = instance_create(x, y, blockBullet);
with (s)
   {
   speed = 20;
   direction = 315;
   }
}
//////////////////////////////////////////////////////
// Movement
var moveSpeed, sprintTimer;
moveSpeed = 4;

// dash
if (keyboard_check(vk_shift) || gamepad_button_value(0, gp_shoulderl))
{
    audio_play_sound(SprintEngaged, 10, false);
    global.Energy -= 1;
    x += h_move * 8;
    y += v_move * 8;
}
    
if keyboard_check(ord('W')) y -= moveSpeed;
if keyboard_check(ord('S')) y += moveSpeed;
if keyboard_check(ord('A')) x -= moveSpeed;
if keyboard_check(ord('D')) x += moveSpeed;

// Diagonal Movement 
if (keyboard_check(ord("D")) &amp;&amp; keyboard_check(ord("W")))  motion_set(45 ,moveSpeed);
    else if (keyboard_check(ord("A")) &amp;&amp; keyboard_check(ord("W")))  motion_set(135,moveSpeed);
    else if (keyboard_check(ord("A")) &amp;&amp; keyboard_check(ord("S")))  motion_set(225,moveSpeed); 
    else if (keyboard_check(ord("S")) &amp;&amp; keyboard_check(ord("D")))  motion_set(315,moveSpeed);
    else if (keyboard_check(ord("A"))) motion_set(180, moveSpeed);
    else if (keyboard_check(ord("S"))) motion_set(270, moveSpeed);
    else if (keyboard_check(ord("D"))) motion_set(0, moveSpeed);
    else if (keyboard_check(ord("W"))) motion_set(90, moveSpeed);
    else if (keyboard_check(vk_nokey)) motion_set(0,0);
    
// boundry
x = clamp(x, 0, room_width); 
y = clamp(y, 0, room_height); 
</string>
          </argument>
        </arguments>
      </action>
